Ninja And The Sorted Check

bool isSortedArr(int n, vector<int> a){
    for(int i=1; i<n; i++){
        if(a[i] >= a[i-1]){

        }else{
            return false;
        }
    }return true;
}


int isSorted(int n, vector<int> a) {
    return isSortedArr(n,a);
}
